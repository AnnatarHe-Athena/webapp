  "private": true,
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.1",
    "apollo-client": "^2.0.3",
    "apollo-link": "^1.0.3",
    "apollo-link-context": "^1.0.1",
    "apollo-link-error": "^1.0.1",
    "apollo-link-http": "^1.2.0",
    "graphql": "^0.11.7",
    "immutable": "^3.8.1",
    "intersection-observer": "^0.4.0",
    "rc-notification": "^3.2.0",
    "react": "^16",
    "react-apollo": "^2.0.1",
    "react-dom": "^16",
    "react-hot-loader": "^4.0.0",
    "react-redux": "^5.0.5",
    "react-router": "3",
    "react-router-redux": "^4.0.8",
    "react-scripts": "1.0.10",
    "react-select": "^1.0.0-rc.10",
    "react-spinkit": "^3.0.0",
    "react-transition-group": "1.x",
    "redux": "^3.7.2",
    "redux-immutable": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.15.6",
    "styled-components": "^2.1.1",
    "whatwg-fetch": "^2.0.3"
  },
  "scripts": {
    "dev:dll": "cross-env NODE_ENV=development webpack --config scripts/webpack.dll.config.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config scripts/webpack.dev.config.js",
    "prod:dll": "cross-env NODE_ENV=production webpack -p --config scripts/webpack.dll.config.js",
    "prod": "cross-env NODE_ENV=production webpack -p --config scripts/webpack.prod.config.js",
    "mock": "json-server mock/db.js --routes mock/routes.json --port 9000",
    "lint": "eslint --ext .jsx --ext .js src/",
    "lint:fix": "eslint --ext .jsx --ext .js --fix src/",
    "build": "npm run prod:dll && npm run prod && bestzip dist.zip dist/",
    "release:base": "standard-version --commit-all",
    "release": "npm run release:base --",
    "release:pre": "npm run release:base -- --prerelease alpha"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^2.1.2",
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-styled-components": "^1.1.7",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "bestzip": "^1.1.4",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.5",
    "eslint": "^4.9.0",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.0.0",
    "graphql-tag": "^2.4.2",
    "html-webpack-plugin": "^2.30.1",
    "offline-plugin": "^4.8.4",
    "postcss-normalize": "^4.0.0",
    "poststylus": "^1.0.0",
    "standard-version": "^4.3.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.7.1",
    "webpack-pwa-manifest": "^3.3.2"
  }
