    "react-loadable": "^5.5.0",
    "react-redux": "^6.0.0",
    "react-scripts": "2.1.3",
    "react-spinkit": "^3.0.0",
    "react-transition-group": "2.5.2",
    "redux": "^4.0.1",
    "redux-immutable": "^4.0.0",
